<h3>Student Form</h3>

<%= form_tag students_path do %>
  <label>Student title:</label><br>
  <%= text_field_tag :'student[title]' %><br>

  <label>Student description:</label><br>
  <%= text_area_tag :'student[description]' %><br>

  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>
